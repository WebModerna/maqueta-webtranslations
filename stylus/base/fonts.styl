@font-face {
	font-family: 'Open Sans Condensed';
	font-style: normal;
	font-weight: 300;
	src: url(fonts/OpenSansCondensed.eot);
	src: url(fonts/OpenSansCondensed.eot) format('embedded-opentype'),
	local('Open Sans Cond Light'), local('OpenSans-CondensedLight'), url(fonts/OpenSansCondensed.woff2) format('woff2'),
	local('Open Sans Cond Light'), local('OpenSans-CondensedLight'), url(fonts/OpenSansCondensed.woff) format('woff'),
	local('Open Sans Cond Light'), local('OpenSans-CondensedLight'), url(fonts/OpenSansCondensed.svg#OpenSansCondensed) format('svg'),
	local('Open Sans Cond Light'), local('OpenSans-CondensedLight'), url(fonts/OpenSansCondensed.ttf) format('truetype');
}
